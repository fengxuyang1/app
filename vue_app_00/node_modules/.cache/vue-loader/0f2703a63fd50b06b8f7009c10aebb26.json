       <span>点击{{item.point}}次</span>\r\n                </p>\r\n            </div>\r\n        </router-link>\r\n    </li> \r\n</ul>\r\n<mt-button type=\"primary\" size=\"large\" @click=\"getMore\">加载更多...</mt-button>\r\n</div>\r\n</template>\r\n<script>\r\n    export default {\r\n        data(){\r\n            return {\r\n                list:[],\r\n                pno:1,     //当前页码\r\n                pageSize:7 //页大小\r\n            }\r\n        },\r\n        methods:{\r\n            //加载更多 加载下一页数据\r\n            getMore(){\r\n                //1 修改当前页码+1\r\n                this.pno++;\r\n                //2 发送请求get\r\n                var url=\"http://127.0.0.1:3000/slist?pno=\"+this.pno+\"&pageSize=\"+this.pageSize;\r\n                //3 获取下一页数据保存 list\r\n                this.axios.get(url).then(result=>{\r\n                    //3.1 拼接二个数组\r\n                    var rows=this.list.concat(result.data.data);\r\n                    //3.2 赋值list\r\n                    this.list=rows;  \r\n                    //this.list=result.data.data;\r\n                })\r\n            },\r\n            //mui查找 左面图片右面文字\r\n            getnewslist(){\r\n                var url=\"http://127.0.0.1:3000/slist\";\r\n                this.axios.get(url).then(result=>{\r\n                  //console.log(result.data.data);\r\n                  this.list=result.data.data;\r\n                })\r\n            }\r\n        },\r\n        created() {\r\n            this.getnewslist();\r\n            //console.log(this.$route.query.nid);\r\n        }\r\n    }\r\n</script>\r\n<style>\r\n    /*日期和点击次数两端对齐*/\r\n    .app-newlist li .mui-ellipsis{\r\n        display:flex;\r\n        font-size:12px;\r\n        color:#226aff;\r\n        justify-content:space-between;\r\n    }\r\n</style>"]}]}